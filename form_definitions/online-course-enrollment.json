{
  "title": "Online Course Enrollment",
  "description": "Enroll in our comprehensive online learning programs",
  "submitEndpoint": "https://jsonpost.com/api/submit/63c61c56-f128-4096-a818-771ee0bafd0c/studyhop-wizard-form",
  "steps": [
    {
      "id": "welcome",
      "type": "statement",
      "title": "Welcome to Our Learning Platform",
      "content": "Transform your career with our expert-led courses. Complete this enrollment form to get started on your learning journey!",
      "required": false
    },
    {
      "id": "student_name",
      "type": "short_text",
      "title": "Full Name",
      "placeholder": "Enter your full name as it should appear on certificates",
      "required": true,
      "validation": {
        "minLength": 2,
        "maxLength": 100
      }
    },
    {
      "id": "email",
      "type": "email",
      "title": "Email Address",
      "placeholder": "your.email@example.com",
      "required": true,
      "validation": {
        "pattern": "^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$"
      }
    },
    {
      "id": "phone",
      "type": "phone",
      "title": "Phone Number",
      "placeholder": "+1 (555) 123-4567",
      "required": false
    },
    {
      "id": "birth_date",
      "type": "date",
      "title": "Date of Birth",
      "required": true,
      "validation": {
        "maxDate": "today"
      }
    },
    {
      "id": "education_level",
      "type": "dropdown",
      "title": "Highest Education Level",
      "required": true,
      "options": [
        { "value": "high_school", "label": "High School" },
        { "value": "associate", "label": "Associate Degree" },
        { "value": "bachelor", "label": "Bachelor's Degree" },
        { "value": "master", "label": "Master's Degree" },
        { "value": "doctorate", "label": "Doctorate/PhD" },
        { "value": "other", "label": "Other" }
      ]
    },
    {
      "id": "course_selection",
      "type": "single_select",
      "title": "Which course are you enrolling in?",
      "required": true,
      "options": [
        { "value": "web_dev_bootcamp", "label": "Full Stack Web Development Bootcamp" },
        { "value": "data_science", "label": "Data Science & Analytics" },
        { "value": "digital_marketing", "label": "Digital Marketing Mastery" },
        { "value": "ui_ux_design", "label": "UI/UX Design Fundamentals" },
        { "value": "python_programming", "label": "Python Programming for Beginners" },
        { "value": "project_management", "label": "Project Management Professional" },
        { "value": "cybersecurity", "label": "Cybersecurity Essentials" },
        { "value": "ai_machine_learning", "label": "AI & Machine Learning" }
      ]
    },
    {
      "id": "learning_goals",
      "type": "multiple_select",
      "title": "What are your learning goals?",
      "required": true,
      "options": [
        { "value": "career_change", "label": "Career change" },
        { "value": "skill_upgrade", "label": "Upgrade current skills" },
        { "value": "promotion", "label": "Get promoted at work" },
        { "value": "freelancing", "label": "Start freelancing" },
        { "value": "startup", "label": "Launch my own startup" },
        { "value": "personal_interest", "label": "Personal interest/hobby" },
        { "value": "certification", "label": "Get certified" },
        { "value": "networking", "label": "Professional networking" }
      ]
    },
    {
      "id": "experience_level",
      "type": "single_select",
      "title": "What's your experience level in this field?",
      "required": true,
      "options": [
        { "value": "complete_beginner", "label": "Complete beginner" },
        { "value": "some_knowledge", "label": "Some basic knowledge" },
        { "value": "intermediate", "label": "Intermediate" },
        { "value": "advanced", "label": "Advanced" }
      ]
    },
    {
      "id": "time_commitment",
      "type": "dropdown",
      "title": "How many hours per week can you dedicate to learning?",
      "required": true,
      "options": [
        { "value": "1-3", "label": "1-3 hours" },
        { "value": "4-6", "label": "4-6 hours" },
        { "value": "7-10", "label": "7-10 hours" },
        { "value": "11-15", "label": "11-15 hours" },
        { "value": "16-20", "label": "16-20 hours" },
        { "value": "20+", "label": "20+ hours" }
      ]
    },
    {
      "id": "preferred_schedule",
      "type": "multiple_select",
      "title": "When do you prefer to study?",
      "required": false,
      "options": [
        { "value": "early_morning", "label": "Early morning (6-9 AM)" },
        { "value": "morning", "label": "Morning (9 AM-12 PM)" },
        { "value": "afternoon", "label": "Afternoon (12-5 PM)" },
        { "value": "evening", "label": "Evening (5-8 PM)" },
        { "value": "night", "label": "Night (8-11 PM)" },
        { "value": "late_night", "label": "Late night (11 PM+)" },
        { "value": "weekends", "label": "Weekends only" }
      ]
    },
    {
      "id": "learning_style",
      "type": "single_select",
      "title": "What's your preferred learning style?",
      "required": false,
      "options": [
        { "value": "video_lectures", "label": "Video lectures" },
        { "value": "hands_on_projects", "label": "Hands-on projects" },
        { "value": "reading_materials", "label": "Reading materials" },
        { "value": "interactive_exercises", "label": "Interactive exercises" },
        { "value": "group_discussions", "label": "Group discussions" },
        { "value": "mixed_approach", "label": "Mixed approach" }
      ]
    },
    {
      "id": "motivation_level",
      "type": "opinion_scale",
      "title": "How motivated are you to complete this course?",
      "required": true,
      "scale": {
        "min": 1,
        "max": 10,
        "minLabel": "Not very motivated",
        "maxLabel": "Extremely motivated"
      }
    },
    {
      "id": "support_needs",
      "type": "multiple_select",
      "title": "What kind of support would be most helpful?",
      "required": false,
      "options": [
        { "value": "mentor_access", "label": "Access to mentors" },
        { "value": "peer_community", "label": "Peer community/forums" },
        { "value": "career_guidance", "label": "Career guidance" },
        { "value": "technical_support", "label": "Technical support" },
        { "value": "study_groups", "label": "Study groups" },
        { "value": "office_hours", "label": "Instructor office hours" },
        { "value": "none", "label": "I prefer self-study" }
      ]
    },
    {
      "id": "expectations",
      "type": "long_text",
      "title": "What do you hope to achieve by the end of this course?",
      "placeholder": "Describe your specific goals and expectations...",
      "required": false,
      "validation": {
        "maxLength": 500
      }
    }
  ]
}